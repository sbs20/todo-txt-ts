{
  "name": "todo-txt-ts",
  "version": "1.0.3",
  "description": "Small Typescript API to todotxt format",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "types": "tsc --project tsconfig.types.json",
    "build": "rollup -c",
    "test": "mocha -r ts-node/register test/**/*.*s",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "all": "npm run lint && npm run test && npm run build && npm run types"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sbs20/todo-txt-ts.git"
  },
  "keywords": [
    "todotxt",
    "typescript"
  ],
  "author": "Sam Strachan",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "11.1.1",
    "@rollup/plugin-typescript": "8.1.1",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "4.2.0",
    "chai": "^4.2.0",
    "del": "6.0.0",
    "eslint": "7.19.0",
    "mocha": "^8.2.1",
    "rollup": "2.38.5",
    "rollup-plugin-terser": "^7.0.2",
    "ts-mocha": "8.0.0",
    "ts-node": "9.1.1",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist/",
    "src/",
    "package.json",
    "package-lock.json",
    "README.md",
    "LICENSE"
  ]
}
